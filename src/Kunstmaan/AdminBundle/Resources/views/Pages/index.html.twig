{% extends 'KunstmaanAdminBundle:Default:layout.html.twig' %}

{% block sidebar %}
<div class="sidebar">
    <div id="tree">
        <ul>
            {% for topnode in topnodes %}
                {% include 'KunstmaanAdminBundle:Pages:rectreeview.html.twig' %}
            {% endfor %}
        </ul>
    </div>
</div>
{% endblock %}

{% block breadcrumb %}
	<ul class="breadcrumb">
		<li><a href="{{ path('KunstmaanAdminBundle_homepage') }}">Home</a> <span class="divider">/</span></li>
		<li class="active">Pages</li>
	</ul>
{% endblock %}

{% block content %}

    <div class="page-header">
        <h1>Pages</h1>
    </div>

    {{ adminlist_widget(pageadminlist, 'KunstmaanAdminBundle_pages') }}

{% endblock %}
